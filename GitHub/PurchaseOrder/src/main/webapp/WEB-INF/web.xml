<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetOrders</servlet-name>
  	<servlet-class>com.company.controller.GetOrders</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SaveOrders</servlet-name>
  	<display-name>SaveOrders</display-name>
  	<description></description>
  	<servlet-class>com.company.controller.SaveOrders</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SecurityKeyCheck</servlet-name>
  	<display-name>SecurityKeyCheck</display-name>
  	<description></description>
  	<servlet-class>com.company.controller.SecurityKeyCheck</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>GetApproval</servlet-name>
  	<display-name>GetApproval</display-name>
  	<description></description>
  	<servlet-class>com.company.controller.GetApproval</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetOrders</servlet-name>
  	<url-pattern>/GetOrders</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SaveOrders</servlet-name>
  	<url-pattern>/SaveOrders</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SecurityKeyCheck</servlet-name>
  	<url-pattern>/SecurityKeyCheck</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>GetApproval</servlet-name>
  	<url-pattern>/GetApproval</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>form.jsp</welcome-file>
    <welcome-file>SaveOrders</welcome-file>
    <welcome-file>home.jsp</welcome-file>
</welcome-file-list>
</web-app>
