<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CallbackServlet</servlet-name>
  	<display-name>CallbackServlet</display-name>
  	<description></description>
  	<servlet-class>com.company.xero.controller.CallbackServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RequestResourceServlet</servlet-name>
  	<display-name>RequestResourceServlet</display-name>
  	<description></description>
  	<servlet-class>com.company.xero.controller.RequestResourceServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>RequestTokenServlet</servlet-name>
  	<display-name>RequestTokenServlet</display-name>
  	<description></description>
  	<servlet-class>com.company.xero.controller.RequestTokenServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CallbackServlet</servlet-name>
  	<url-pattern>/CallbackServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestResourceServlet</servlet-name>
  	<url-pattern>/RequestResourceServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>RequestTokenServlet</servlet-name>
  	<url-pattern>/RequestTokenServlet</url-pattern>
  </servlet-mapping>
  
  
  
  
  
  
 
  
</web-app>
